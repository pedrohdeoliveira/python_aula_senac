# Comandos de repetição
for x in range (5,11):
    print(x)
ListaNomes = ["Maria", "Ana", "Pedro","João"]
for x in ListaNomes:
    print(x)
numeros = (3,6,4,9)
soma = 0
for x in numeros:
    soma += x 
print (soma)
i=0
while i<10:
    print(i)
    i+=1
lista = ["coca", "fanta", "sprite", "tai", "elefante", "foca"]
i=0
while i < len(lista):
    #print(i," - " [i])
    print (i, " - ", lista [i])
    i+=1